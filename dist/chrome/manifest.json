{
  "manifest_version": 2,

  "name": "StarCitizen.FR Discord Extension",
  "description": "Enhances Discord with SC.FR features.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "static/icon.png",
    "default_popup": "static/popup.html"
  },
  "permissions": [],
  
  "content_scripts": [
  {
    "matches": ["https://discordapp.com/*"],
    "js": ["StarCitizenFR.plugin.js", "js/main.js"]
  }
]
}
